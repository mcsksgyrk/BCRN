digraph Autophagy_Apoptosis_Network {
	bgcolor=white rankdir=LR size=10
	subgraph cluster_autophagy {
		color=lightblue label=Autophagy style=filled
		ATG5
		BECN1
		UVRAG
		PHAG
		AUT
	}
	subgraph cluster_apoptosis {
		color=lightyellow label=Apoptosis style=filled
		P53
		BAX
		CASP
		CYTC
		TBID
	}
	subgraph cluster_calcium {
		color=lightcyan label=Calcium style=filled
		"Ca2+ (IC)"
		"Ca2+ (ER)"
		CAMKK
		AMPK
	}
	subgraph cluster_inositol {
		color=mistyrose label=Inositol style=filled
		IP3
		PLC
		CAMP
	}
	subgraph cluster_mtor {
		color=honeydew label=mTOR style=filled
		MTOR
		ULK1
		RHEB
		TSC
	}
	P53 -> BAX
	BAX -> CYTC
	CYTC -> CASP
	CASP -> TBID
	BECN1 -> UVRAG
	UVRAG -> PHAG
	PHAG -> AUT
	"Ca2+ (IC)" -> CAMKK
	CAMKK -> AMPK
	AMPK -> ULK1
	IP3 -> "Ca2+ (IC)" [label=release]
}
