<experiment>
    <fileAuthor>Bence Hajdu and Tibor Nagy</fileAuthor>
    <fileVersion>
        <major>1</major>
        <minor>0</minor>
    </fileVersion>
    <ReSpecThVersion>
        <major>2</major>
        <minor>5</minor>
    </ReSpecThVersion>
    <bibliographyLink>
        <description></description>
        <details>
            <author>{{ bib[author] }}</author>
            <journal>{{ bib[journal] }}</journal>
            <number>{{ bib[number] }}</number>
            <title>{{ bib[title] }}</title>
            <volume>{{ bib[volume] }}</volume>
            <year>{{ bib[year] }}</year>
        </details>
    </bibliographyLink>
    <experimentType>concentration time profile measurement</experimentType>
    <apparatus>
        <kind>flow reactor</kind>
    </apparatus>
    <commonProperties>
        <property name="temperature"          sourcetype="reported"   units="K"       ><value>309.15</value></property>
        <property name="pressure"             sourcetype="reported"   units="bar"     ><value>25705</value></property>
        <property name="initial composition"  sourcetype="digitized" >
            {%- for k,v in ics.items() %}
            <component><speciesLink preferredKey="{{k}}"         chemName="{{k}}"     /><amount units="mole fraction" >{{v}}</amount></component>
            {%- endfor %}
        </property>
        {%- for k,v in output_sigmas.items() %}
        <property name="evaluated standard deviation"  sourcetype="reported"  units="mole fraction"  reference="composition"  kind="absolute"  sigmarange="1"  method="statistical scatter" >
            <speciesLink preferredKey="{{k}}"  chemName="{{k}}" />
            <value>"{{v}}"</value>
        </property>
        {%- endfor %}
    </commonProperties>
    <dataGroup id="dg1">
        <property name="time"                          id="time"    label="t"       sourcetype="reported"  units="min"     />
        {%- for name in variables %}
        <property name="composition"                   id="{{name}}"   label="{{name}}"   sourcetype="reported"  units="mole fraction" >
            <speciesLink preferredKey="{{name}}"  chemName="{{name}}" />
        </property>
        {%- endfor %}
        {%- for dp in dataPoints %}
            {{dp}}
        {%- endfor %}
    </dataGroup>
</experiment>
